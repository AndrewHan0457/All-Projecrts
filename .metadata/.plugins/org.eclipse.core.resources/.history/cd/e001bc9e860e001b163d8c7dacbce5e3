 */
package Customers;

import InterfacesAndEnums.Filter;
import InterfacesAndEnums.Mode;

/**
 * @author Andrew Han
 *
 */
public class Customer {
	private String name;
	private int age;
	private String tel;
	private Property property;
	private Mode mode;
	
	public static final account = "Tom";
	public static final pwd = "";
	
	public Customer(String name) {
		this.name = name;
	}
	
	public void loginAndSignIn(String account, String pwd) {
		UserInterface ui = new UserInterface(this);
		ui.loginOrSignIn(this);
	}
	
	public void search(Filter [] f) {
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public Property getProperty() {
		return property;
	}

	public void setProperty(Property property) {
		this.property = property;
	}

	public Mode getMode() {
		return mode;
	}

	public void setMode(Mode mode) {
		this.mode = mode;
	}
	
	
}
